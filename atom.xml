---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>brendan forster</title>
 <link href="http://shiftkey.heroku.com/atom.xml" rel="self" />
 <link href="http://shiftkey.heroku.com/" />
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>shiftkey.heroku.com<id>
 <author>
   <name>site.owner-name</name>
   <email>site.owner-email</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://your.pages.url{{ post.url }}" />
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://shiftkey.heroku.com/{{ post.id }}</id>
   <content type="html">
	<![CDATA[{{ post.content  }}]]>
   </content>
   
 </entry>
 {% endfor %}
 
</feed>